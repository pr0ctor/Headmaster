@model Headmaster.Models.Registrations

@{
    ViewBag.Title = "Delete";
}

<h2>Delete</h2>

<h3>Are you sure you want to delete this?</h3>
<div>
    <h4>Registrations</h4>
    <hr />
    <dl class="dl-horizontal">
        <dd>
            @Html.DisplayFor(model => model.AvailableCourses.Courses.Departments.Abbreviation) @Html.DisplayFor(model=>model.AvailableCourses.Courses.CourseNumber)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.AvailableCourses.Courses.CourseName)
        </dt>

        <dd>
           Credits:  @Html.DisplayFor(model => model.AvailableCourses.Courses.Credits)
        </dd>

        <dt>
           Description:  @Html.DisplayNameFor(model => model.AvailableCourses.Courses.Description)
        </dt>

        

    </dl>

    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Delete" class="btn btn-default" /> |
            @Html.ActionLink("Back to List", "Index")
        </div>
    }
</div>
